{
  "name": "actual-build",
  "version": "1.0.0",
  "description": "A comprehensive full stack development environment designed for building modern web applications with ease.",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm run test",
    "test:backend": "cd backend && npm run test",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "install:all": "cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules"
  },
  "keywords": [
    "fullstack",
    "web-development",
    "nodejs",
    "javascript",
    "typescript",
    "build-environment"
  ],
  "author": "Nibert Investments",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nibertinvestments/Actual-build.git"
  },
  "bugs": {
    "url": "https://github.com/nibertinvestments/Actual-build/issues"
  },
  "homepage": "https://github.com/nibertinvestments/Actual-build#readme"
}